{
  "name": "recordly",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main/server.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "npm run build -- --watch",
    "start": "node dist/main/server.js",
    "dev": "nodemon -L --watch ./dist ./dist/main/server.js",
    "start:dev": "concurrently --kill-others-on-fail \"npm run build:watch\" \"npm run dev\"",
    "test": "jest --passWithNoTests --no-cache",
    "test:stage": "npm run test -- --findRelatedTests",
    "test:ci": "npm run test -- --coverage",
    "lint": "estlint --ext .ts src/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "pg": "^8.10.0",
    "typeorm": "^0.3.15"
  },
  "_moduleAliases": {
    "@domain": "dist/domain",
    "@application": "dist/application",
    "@infrastructure": "dist/infrastructure",
    "@main": "dist/main"
  }
}
